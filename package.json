{
  "name": "resumate",
  "version": "1.0.2",
  "description": "AI-powered experience structuring tool for resume building",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "resumate": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && node -e \"const fs=require('fs-extra');fs.copySync('src/templates/skills','dist/templates/skills')\"",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "local": "npm run build && node dist/cli/index.js",
    "playground:clean": "node -e \"const fs=require('fs-extra');fs.existsSync('playground')&&fs.removeSync('playground');fs.mkdirSync('playground')\"",
    "playground:init": "npm run playground:clean && npm run build && cd playground && node ../dist/cli/index.js init test-project",
    "playground:demo": "node scripts/demo-flow.cjs"
  },
  "keywords": [
    "resume",
    "cli",
    "ai",
    "career",
    "experience",
    "markdown"
  ],
  "author": "taeyeongryu",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "commander": "^14.0.2",
    "date-fns": "^3.3.1",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.5",
    "eslint": "^9.0.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0",
    "vitest": "^1.2.1"
  }
}
